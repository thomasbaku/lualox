// function to calculate CS 403 grade
// Thomas Hampton

fun gradebook(midterm, proj1, proj2, projFinal, hs1, hs2, hs3, hs4, hs5, final)
{
  proj = ((proj1 + proj2) / 2) * .3;
  other = ((hs1 + hs2 + hs3 + hs4 + hs5) / 5) * .2;

  return proj + other + (midterm * .2) + (projFinal * .2) + (final * .1);
}

